{
    "name": "softlabs/packages",
    "description": "A collection of Softlabs Packages",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Erskine",
            "email": "matthew.erskine@softlabs.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "replace": {
        "softlabs/breadcrumb": "self.version",
        "softlabs/currency": "self.version",
        "softlabs/date": "self.version",
        "softlabs/util": "self.version",
        "softlabs/validator": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master@dev",
        "orchestra/testbench": "2.0.*"
    },
    "autoload": {
        "psr-0": {
            "Softlabs": "src/"
        }
    },
    "minimum-stability": "dev"
}